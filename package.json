{
  "name": "marcosalves.io",
  "description": "marcosalves.io",
  "version": "0.0.1",
  "engines": {
    "node": "14.x",
    "npm": "6.7.0"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "clean": "rm -rf ./__sapper__",
    "export": "sapper export",
    "deploy": "npm run clean && npm run export && node ./scripts/deploy.js",
    "prod": "npm run build && npm run start"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "gh-pages": "^2.2.0",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0",
    "svelte-typewriter": "^2.1.9",
    "webpack": "^4.7.0"
  }
}
